                <div class="col-md-4 form-group" style="padding-left:0px;">
                    <div class="col-md-6" style="padding-right: 0px"><span class="dtl_label">Payout</span><span class="dtl_value money-dtl">@Html.EditorFor(m => m.vM_AddLoadData.Payout, new { htmlAttributes = new { @class = "form-control dtl_number dtl_number pull-left", @style = "width:100%" } })</span></div>
                    <div class="col-md-6" style="padding-right: 0px"><span class="dtl_label">Price PM</span><span class="dtl_value money-dtl">@Html.EditorFor(m => m.vM_AddLoadData.Price_Per_Mile, new { htmlAttributes = new { @class = "form-control dtl_number dtl_number pull-left", @style = "width:100%", @disabled = true } })</span></div>
                </div>
            </div>
            <div class="col-md-12">
                <div class="col-md-4 form-group"><span class="dtl_label">Weight : </span><span id="payout-dtl" class="dtl_value padding-right-md money-round-dtl">@Html.EditorFor(m => m.vM_AddLoadData.Weight, new { htmlAttributes = new { @class = "form-control dtl_number" } })</span></div>
                <div class="col-md-4 form-group"><span class="dtl_label">Contents : </span><span class="dtl_value padding-right-md money-dtl">@Html.EditorFor(m => m.vM_AddLoadData.Contents, new { htmlAttributes = new { @class = "form-control" } })</span></div>
                <div class="col-md-4 form-group" style="padding-left:0px;">
