<form class="loadaddform" enctype="multipart/form-data">
    <div id="addLoad_InfoBox" class="card border border-secondary LoadInfoBox">
        <div class="card-header card-header-addLoad">
            <div class="col-md-4 form-group"><span class="dtl_value padding-right-md">Load Number :</span><span id="vM_AddLoadData_LoadNbr" class="dtl_value padding-right-md">@Html.EditorFor(m => m.vM_AddLoadData.Load_Number, new { htmlAttributes = new { @class = "form-control dtl_number" } })</span><span style="color:white;"><small>Load Number cannot be edited</small></span></div>
            <div class="col-md-4 form-group"><span class="dtl_value padding-right-md">Customer :</span><span class="dtl_value padding-right-md">@Html.DropDownListFor(m => m.vM_AddLoadData.Customer_ID, new SelectList(Model.AddLoad_Customer_List, "Id", "Value"), new { @class = "form-control" })</span></div>
            <div class="col-md-4 form-group no-pad-right">
                <span class="dtl_value ">Load Confirmation :</span>
                <div class="col-md-12 no-pad-horizontal">
                    <div class="col-md-8 no-pad-horizontal">
                        <span id="addLoad-uploadbox" class="btn btn-basic fileinput-button fileInput-maxx-disabled">
                            <span id="AddLoad_LoadConfirmation_Filename" class="dtl_number">Select File</span>
                            <input id="AddLoad_LoadConfirmation_Upload" name="AddLoad_LoadConfirmation_Upload" class="LoadConfirmationUpload" type="file" disabled onchange="get_filename()">
                        </span>
                        <div id="addLoad_UploadProgress" class=""></div>
                    </div>
                    <div class="col-md-4 no-pad-horizontal">
                        <span class="btn btn-primary-disabled fileinput-button AddLoad_LoadConfirmationUpload_Submit" style="width: 100%">
                            <span>Upload</span>
                            <input class="AddLoad_LoadConfirmationUpload_Submit LoadConfirmationUpload" type="button">
                        </span>
                    </div>
                </div>
            </div>
        </div>
        <div class="card-body">

            <div class="col-md-12">
                <div class="col-md-4 form-group"><span class="dtl_label">Week of : </span><span class="dtl_value"> @Html.DropDownListFor(m => m.vM_AddLoadData.Week_of, new SelectList(Model.AddLoad_Weekof_List, "Id", "Value"), new { @class = "form-control dtl_number" })</span></div>
