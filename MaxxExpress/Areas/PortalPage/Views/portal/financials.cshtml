@using System.Web.Http
@using System.Web.Http.Controllers
@using System.Web.Http.Description
@using System.Collections.ObjectModel
@using MaxxExpress.Areas.WorkPage.Models.Tools;

@model MaxxExpress.Areas.PortalPage.Models.PortalViewModel

@{

    ViewBag.Title = "Maxx Express - P & L Dashboard";
}
<!-- Begin Left Panel -->
@{
    Html.RenderPartial("_FinancialsAreaLeft_Admin");
}
<!-- End Left Panel -->
<!-- Begin Right Panel -->
@{
    Html.RenderPartial("_FinancialsAreaRight_Admin");
}
<!-- EndRight Panel -->
<!--Start Show Session Expire Warning Popup here -->

<div class="modal fade" id="session-expire-warning-modal" aria-hidden="true" data-keyboard="false" data-backdrop="static" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Session Expired Message</h4>
            </div>
            <div class="modal-body">
                Your session has expired please log back in.
            </div>
            <div class="modal-footer">
                @*<button id="btn-LogoutNow" type="button" class="btn btn-outline-danger" style="padding: 6px 12px; margin-bottom: 0; font-size: 14px; font-weight: normal; ">Logout now</button>*@

            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="financialLoading_Modal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" style="display: none;">
    <div class="modal-dialog" role="document">
        <div class="modal-content-not-needed">
            <div class="modal-body" style="min-height:200px">
                <div class="col-md-12">
                    <div class="col-4"></div>
                    <div class="col-4" style="margin-left:auto; margin-right:auto;">
                        <div class="loading-processing"></div>
                        <div id="sending-status" class="text-center"></div>
                    </div>
                   
                </div>
            </div>

        </div>
    </div>
</div>

<script src="//code.jquery.com/jquery-1.10.2.js"></script>
<script type="text/javascript">

    $(function () {


                    $("#NET-PNL-Value").load('../Work/reloadFinancials_NET');
                    $("#Load-Revenue-Value").load('../Work/reloadFinancials_Revenue');
                    $("#Fuel-Total-Value").load('../Work/reloadFinancials_Fuel');
                    $("#Payroll-Total-Value").load('../Work/reloadFinancials_Payroll');
                    $("#Expenses-Total-Value").load('../Work/reloadFinancials_Expenses');

      
       
    });
</script>

<!--End Show Session Expire Warning Popup here -->